fun main() {
    print("Digita el lado 1")
    val lado1= readLine()
    print("Digita el lado 2")
    val lado2= readLine()
    print("Digita el lado 3")
    val lado3= readLine()

    if(lado1 == lado2 && lado2==lado3 && lado3==lado1){
        print("Es equilatero")
    }else{
        if(lado1== lado2 || lado2==lado3 || lado1==lado3){
            print("Es is√≥sceles")
        }else{
            print("Es escaleno")
        }
    }
}
